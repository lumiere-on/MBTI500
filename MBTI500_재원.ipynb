        "  for types in pers_types:\n",
        "    data = data.replace(types, '')\n",
        "\n",
        "after_post = []\n",
        "for post in MBTI['posts']:\n",
        "  after_post.append(preprocess(post))\n",
        "after_post[:5]\n",
        "\n",
        "MBTI['posts']=after_post\n",
